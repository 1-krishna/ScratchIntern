<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent"
  android:background="@android:color/holo_blue_bright">

  <TextView
    android:id="@+id/dummyTextView"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="Another Fragment"
    android:textSize="30sp"
    android:layout_centerHorizontal="true"
    android:layout_marginTop="100dp" />

  <TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/anotherFragmentTextOne"
    android:layout_below="@id/dummyTextView"
    android:layout_marginTop="30dp"
    android:layout_centerHorizontal="true"
    android:text="Data 1 from dashboard"
    android:textSize="20sp"/>

  <TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/anotherFragmentTextTwo"
    android:layout_below="@id/anotherFragmentTextOne"
    android:layout_marginTop="30dp"
    android:layout_centerHorizontal="true"
    android:text="Data 2 from dashboard"
    android:textSize="20sp"/>



</RelativeLayout>
